{
    "name": "json/jframework",
    "description": "A PHP Framework Inspired By Lumen And Laravel.",
    "keywords": ["framework", "php", "lumen","Laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/container": "^8.77",
        "illuminate/http": "^8.80",
        "symfony/error-handler": "^5.4",
        "illuminate/config": "^8.80",
        "vlucas/phpdotenv": "^5.4",
        "nikic/fast-route": "^1.3",
        "illuminate/database": "^8.80",
        "illuminate/events": "^8.80",
        "illuminate/log": "^8.80"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Laravel\\Lumen\\": "jframework/Lumen/"
        },
        "files": [
            "jframework/Lumen//helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
